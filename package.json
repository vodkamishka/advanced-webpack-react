  "main": "index.js",
  "scripts": {
    "server": "node ./json-server/index.ts",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "start": "webpack serve --env PORT=3000",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "loki": "loki --chromeConcurrency 2",
    "loki:ok": "loki approve",
    "loki:ci": "loki --requireReference --reactUri file:./storybook-static",
    "prepare": "chmod +x ./node_modules/husky/bin.js && husky install"
  },
